{
    "repositories": [{
        "type": "composer",
        "url": "https://composer.melting-media.com"
    }],
    "name": "modx/shell",
    "description": "MODX CLI",
    "keywords": ["MODX", "shell", "sh"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Tripault",
            "email": "romain@melting-media.com",
            "homepage": "http://melting-media.com",
            "role": "Developer"
        }
    ],
    "require": {
        "melting/modx/package": "0.2.*",
        "symfony/console": "^3.1",
        "symfony/process": "^3.1",
        "symfony/finder": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "bin": [
        "bin/modx"
    ],
    "config": {
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "MODX\\Shell\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.3-dev"
        }
    },
    "non-feature-branches": ["_old", "generators"]
}
