{
    "repositories": [{
        "type": "composer",
        "url": "https://composer.melting-media.com"
    }],
    "name": "MODX/Shell",
    "description": "MODX CLI",
    "keywords": ["MODX", "shell", "sh"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Tripault",
            "email": "romain@melting-media.com",
            "homepage": "http://melting-media.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/console": "dev-master",
        "symfony/process": "dev-master",
        "symfony/finder": "dev-master",
        "meltingmedia/cmphelper": "dev-master"
    },
    "scripts": {
        "post-update-cmd": [
            "CmpHelper\\RegisterCommands::run"
        ]
    },
    "minimum-stability": "dev",
    "bin": ["bin/modx"],
    "config": {
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-0": {
            "MODX": "src/"
        }
    }
}
