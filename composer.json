{
    "repositories": [{
        "type": "composer",
        "url": "https://composer.melting-media.com"
    }],
    "name": "MODX/Shell",
    "description": "MODX CLI",
    "keywords": ["MODX", "shell", "sh"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Tripault",
            "email": "romain@melting-media.com",
            "homepage": "http://melting-media.com",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/console": "~2.7",
        "symfony/process": "~2.7",
        "symfony/finder": "~2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "minimum-stability": "dev",
    "bin": [
        "bin/modx"
    ],
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "MODX\\Shell\\": "src/"
        }
    }
}
